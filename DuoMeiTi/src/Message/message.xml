<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="message" namespace="/message" extends="HomepageDefault">
    	<action name="userlist" class="Message.ChatBoardAction" method="getUserList">
    		<result type="json">
    			<param name="includeProperties">
    				adminList\[\d+\]\.username,
    				adminList\[\d+\]\.id,
    				studentList\[\d+\]\.username,
    				studentList\[\d+\]\.id,
    			</param>
    		</result>
    	</action>
    	<action name="reciveMes" class="Message.ChatBoardAction" method="reciveMes">
    		<result name="success" type="json">
    			<param name="includeProperties">
    				error
    			</param>
    		</result>
    	</action>
    </package>
</struts>
